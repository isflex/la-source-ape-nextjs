{
  "name": "gateway",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "dotenvx run -f $FLEX_PROJ_ROOT/env/public/.env.$FLEX_MODE -- next build",
    "lint": "dotenvx run -f $FLEX_PROJ_ROOT/env/public/.env.$FLEX_MODE -- next lint",
    "marp:build": "dotenvx run -f $FLEX_PROJ_ROOT/env/public/.env.$FLEX_MODE -- pnpm marp:html:build",
    "marp:dev": "dotenvx run -f $FLEX_PROJ_ROOT/env/public/.env.$FLEX_MODE -- pnpm marp:html:dev",
    "marp:dev:server": "dotenvx run -f $FLEX_PROJ_ROOT/env/public/.env.$FLEX_MODE -- marp --server --config ./marp.config.mjs",
    "marp:html:build": "cross-env BUILD_RUNNING=true marp --config ./marp.config.mjs",
    "marp:html:dev": "cross-env BUILD_RUNNING=false marp --config ./marp.config.mjs",
    "next:dev": "dotenvx run -f $FLEX_PROJ_ROOT/env/public/.env.$FLEX_MODE -- next dev ./ --port $FLEX_GATEWAY_PORT --hostname $FLEX_GATEWAY_HOSTNAME",
    "next:dev:debug": "FEDERATION_DEBUG='true' NODE_OPTIONS='--inspect' dotenvx run -f $FLEX_PROJ_ROOT/env/public/.env.$FLEX_MODE -- next dev ./ --port $FLEX_GATEWAY_PORT --hostname $FLEX_GATEWAY_HOSTNAME",
    "start": "dotenvx run -f $FLEX_PROJ_ROOT/env/public/.env.$FLEX_MODE -- next start ./ --hostname $FLEX_GATEWAY_DEPLOYED_REMOTE_HOSTNAME"
  },
  "dependencies": {
    "@aws-amplify/adapter-nextjs": "1.4.0",
    "@aws-amplify/ui-react": "^6.7.1",
    "@flex-design-system/framework": "@flex-design-system/framework@0.0.1",
    "@flex-design-system/react-ts": "@flex-design-system/react-ts@0.0.1",
    "@flexiness/domain-store": "@flexiness/domain-store@0.1.0",
    "@flexiness/domain-utils": "@flexiness/domain-utils@0.0.1",
    "@loadable/component": "5.13.2",
    "@module-federation/enhanced": "0.8.5",
    "aws-amplify": "^6.10.2",
    "cors": "2.8.5",
    "debounce": "2.2.0",
    "escape-string-regexp": "5.0.0",
    "motion": "12.4.11",
    "next": "15.2.4",
    "next-recompose-plugins": "3.0.0",
    "react": "19.1.0",
    "react-device-detect": "2.2.3",
    "react-dom": "19.1.0",
    "react-router": "7.1.1",
    "react-sticky-el": "2.1.1"
  },
  "devDependencies": {
    "@aws-amplify/auth-construct": "^1.5.1",
    "@aws-amplify/backend": "^1.13.0",
    "@aws-amplify/backend-auth": "^1.4.2",
    "@aws-amplify/backend-cli": "^1.4.6",
    "@aws-amplify/backend-data": "^1.4.0",
    "@aws-amplify/backend-deployer": "^1.1.13",
    "@aws-amplify/backend-function": "^1.12.0",
    "@aws-amplify/backend-output-schemas": "^1.4.0",
    "@aws-amplify/backend-output-storage": "^1.1.4",
    "@aws-amplify/backend-secret": "^1.1.5",
    "@aws-amplify/backend-storage": "^1.2.4",
    "@aws-amplify/cli-core": "^1.2.1",
    "@aws-amplify/client-config": "^1.5.5",
    "@aws-amplify/core": "6.9.0",
    "@aws-amplify/data-construct": "^1.14.5",
    "@aws-amplify/deployed-backend-client": "^1.5.0",
    "@aws-amplify/form-generator": "^1.0.3",
    "@aws-amplify/model-generator": "^1.0.12",
    "@aws-amplify/platform-core": "^1.5.1",
    "@aws-amplify/plugin-types": "^1.7.0",
    "@aws-amplify/sandbox": "^1.2.9",
    "@aws-amplify/schema-generator": "^1.2.6",
    "@marp-team/marp-cli": "^4.1.1",
    "@svgr/webpack": "8.1.0",
    "@types/cors": "2.8.17",
    "@types/loadable__component": "5.13.4",
    "@types/node": "^20",
    "@types/react": "19.1.0",
    "@types/react-dom": "19.1.2",
    "aws-cdk": "^2.175.1",
    "aws-cdk-lib": "^2.175.1",
    "constructs": "^10.4.2",
    "eslint": "^8",
    "eslint-config-next": "14.1.4",
    "flexiness": "@types/flexiness@0.0.1",
    "sass-embedded": "1.83.0",
    "tsconfig-paths-webpack-plugin": "4.1.0",
    "tsx": "^4.7.2",
    "typescript": "5.7.2",
    "webpack": "5.99.8"
  },
  "peerDependencies": {
    "@aws-sdk/client-s3": "*",
    "@aws-sdk/client-sso-oidc": "*",
    "@aws-sdk/client-sts": "*",
    "@aws-sdk/types": "*",
    "aws-cdk": "*",
    "aws-cdk-lib": "*",
    "constructs": "*",
    "graphql": "*"
  }
}
