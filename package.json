{
  "name": "@flexiness/la_source_ape",
  "version": "1.0.0",
  "private": true,
  "description": "La Source | APE | FullStack NextJS App",
  "repository": "git@github.com:isflex/la-source-ape-nextjs",
  "license": "MIT",
  "author": "ischerer <hello@flexiness.com>",
  "type": "module",
  "scripts": {
    "amplify:info": "pnpm amplify env get --name $(./bin/run-get-env.sh)",
    "build": "cross-env NODE_OPTIONS='--max-old-space-size=4096' pnpm build:client",
    "build:all": "cross-env NODE_OPTIONS='--max-old-space-size=4096' pnpm build:client && pnpm build:gateway",
    "build:bake": "dotenvx run -f $FLEX_PROJ_ROOT/env/public/.env.$FLEX_MODE -- node ./pipeline-ci/build-bake/dist",
    "build:bake:ts": "dotenvx run -f $FLEX_PROJ_ROOT/env/public/.env.$FLEX_MODE -- ts-node --esm ./pipeline-ci/build-bake/src",
    "build:bake:tsc": "tsc -p ./pipeline-ci/build-bake/tsconfig.json --watch --preserveWatchOutput",
    "build:client": "cross-env NODE_OPTIONS='--max-old-space-size=4096' dotenvx run -f $FLEX_PROJ_ROOT/env/public/.env.$FLEX_MODE -- cross-env BUILD_RUNNING=true ./bin/run-build-client.sh",
    "build:gateway": "cross-env NODE_OPTIONS='--max-old-space-size=4096' dotenvx run -f $FLEX_PROJ_ROOT/env/public/.env.$FLEX_MODE -- cross-env BUILD_RUNNING=true turbo run turbo:build:gateway",
    "build:gateway:old": "dotenvx run -f $FLEX_PROJ_ROOT/env/public/.env.$FLEX_MODE -- cross-env BUILD_RUNNING=true ./bin/run-build-gateway.sh",
    "clean:all:tsc": "pnpm exec rimraf -g '{**/dist,**/*.tsbuildinfo}'",
    "clean:compile": "pnpm clean:tsc && pnpm compile:tsc",
    "clean:tsc": "pnpm exec rimraf '{./apps/la-source/ape/on-board/**/dist,./apps/la-source/ape/on-board/**/*.tsbuildinfo,./packages/flex/domain-store/dist,./packages/flex/domain-store/*.tsbuildinfo,./packages/flex/config/aws/dist,./packages/flex/config/aws/*.tsbuildinfo}'",
    "compile": "turbo run compile:clean:sh",
    "compile:clean:sh": "./bin/run-compile.sh clean",
    "compile:sh": "./bin/run-compile.sh",
    "compile:tsc:project:references": "pnpm tsc -b ./tsconfig.build.json",
    "compile:tsc:project:references:watch": "pnpm tsc -b ./tsconfig.build.json --watch --preserveWatchOutput",
    "compile:watch": "turbo run compile:watch:sh",
    "compile:watch:sh": "./bin/run-compile-watch.sh",
    "dev": "dotenvx run -f $FLEX_PROJ_ROOT/env/public/.env.$FLEX_MODE -- cross-env BUILD_RUNNING=false pnpm dev:client",
    "dev:client": "turbo run turbo:dev:client:webpack",
    "dev:gateway": "turbo run turbo:dev:gateway --verbosity=1",
    "init:nx": "pnpm nx init --interactive=0 --nxCloud=1 && pnpm compile:tsup",
    "kill:all:ports": "pnpm kill-port --port 3000,3001,3991,3992,4006,4009,7007,8080,27017",
    "lint": "pnpm lint:fix:turbo",
    "lint:fix:quiet:turbo": "turbo run lint:fix --continue --quiet",
    "lint:fix:turbo": "turbo run lint:fix --continue",
    "lint:flat": "cross-env ESLINT_USE_FLAT_CONFIG=true NODE_OPTIONS='--max-old-space-size=4096' pnpm eslint \"**/src/**/*.{tsx,ts,cts,mts,js,jsx}\" --config ./eslint.config.mjs --no-warn-ignored",
    "lint:flat:file": "cross-env ESLINT_USE_FLAT_CONFIG=true NODE_OPTIONS='--max-old-space-size=4096' pnpm eslint ./apps/la-source/ape/on-board/api/src/fetch/meetingPoint.ts --config ./eslint.config.mjs --no-warn-ignored",
    "lint:legacy": "cross-env ESLINT_USE_FLAT_CONFIG=false NODE_OPTIONS='--max-old-space-size=4096' pnpm eslint \"**/src/**/*.{tsx,ts,cts,mts,js,jsx,cjs,mjs}\" --config ./.eslintrc.cjs --resolve-plugins-relative-to $FLEX_PROJ_ROOT/packages/flex/config/eslint/legacy --debug",
    "lint:legacy:file": "cross-env ESLINT_USE_FLAT_CONFIG=false NODE_OPTIONS='--max-old-space-size=4096' pnpm eslint ./apps/la-source/ape/on-board/api/src/fetch/meetingPoint.ts --config ./.eslintrc.cjs --resolve-plugins-relative-to $FLEX_PROJ_ROOT/packages/flex/config/eslint/legacy",
    "lint:staged": "cross-env ESLINT_USE_FLAT_CONFIG=false NODE_OPTIONS='--max-old-space-size=4096' pnpm lint-staged --config ./.lintstagedrc.mjs",
    "lint:turbo": "turbo run lint:legacy",
    "pnpm:devPreinstall": "./bin/setPkgManagerConfig.sh",
    "precommit": "dotenvx run -f $FLEX_PROJ_ROOT/env/public/.env.$FLEX_MODE -- pnpm lint:fix:quiet:turbo && pnpm lint:staged",
    "prepare": "test -d node_modules/husky && husky || echo \"husky is not installed\"",
    "setup": "pnpm init:nx",
    "start": "pnpm start:client",
    "start:all": "pnpm start:gateway",
    "start:client": "dotenvx run -f $FLEX_PROJ_ROOT/env/public/.env.$FLEX_MODE -- turbo run turbo:start:client",
    "start:gateway": "dotenvx run -f $FLEX_PROJ_ROOT/env/public/.env.$FLEX_MODE -- turbo run turbo:start:gateway"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
