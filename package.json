{
  "name": "@flexiness/la_source_ape",
  "version": "1.0.0",
  "private": true,
  "description": "La Source | APE | FullStack NextJS App",
  "repository": "git@github.com:isflex/la-source-ape-nextjs",
  "license": "MIT",
  "author": "ischerer <hello@flexiness.com>",
  "type": "module",
  "scripts": {
    "ai:claude": "dotenvx run -f $FLEX_PROJ_ROOT/env/public/.env.$FLEX_MODE -- claude --mcp-debug --settings .claude/settings.json",
    "ai:claude:debug": "dotenvx run -f $FLEX_PROJ_ROOT/env/public/.env.$FLEX_MODE -- claude --debug --mcp-debug --settings .claude/settings.json",
    "build": "dotenvx run -f $FLEX_PROJ_ROOT/env/public/.env.$FLEX_MODE -- ./bin/run-build-client.sh",
    "classnames": "^2.5.1",
    "compile": "turbo run compile:clean:sh",
    "compile:clean:sh": "./bin/run-compile.sh clean",
    "compile:sh": "./bin/run-compile.sh",
    "compile:tsc:project:references": "pnpm tsc -b ./tsconfig.build.json",
    "compile:tsc:project:references:watch": "pnpm tsc -b ./tsconfig.build.json --watch --preserveWatchOutput",
    "compile:watch": "turbo run compile:watch:sh",
    "compile:watch:sh": "./bin/run-compile-watch.sh",
    "dev": "dotenvx run -f $FLEX_PROJ_ROOT/env/public/.env.$FLEX_MODE -- turbo run dev",
    "install:prod": "dotenvx run -f $FLEX_PROJ_ROOT/env/public/.env.$FLEX_MODE -- ./bin/run-install-prod.sh",
    "lint": "turbo run lint",
    "prepare": "node .husky/install.mjs",
    "start": "dotenvx run -f $FLEX_PROJ_ROOT/env/public/.env.$FLEX_MODE -- turbo run start",
    "test": "echo \"¯\\_(ツ)_/¯ Will test another day !!\""
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
